#include <stdio.h>

void estaEntre(char caracter){
    if(caracter > 65 && caracter < 122){
        printf("o caracter esta entre A e Z");
    }else{
        printf("O caracter nÃ£o esta entre A e Z");
    }
    
}

int main(){
   char caracter;
   
    printf("Digite um caracter: ");
    scanf("%c",&caracter);
    
    estaEntre(caracter);
    return 0;
}
